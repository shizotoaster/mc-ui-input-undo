{
  "schemaVersion": 1,
  "id": "uiinputundo",
  "version": "${version}",
  "name": "UI Input Undo (Redo)",
  "description": "A client-side Minecraft mod that allows you to undo and redo text in UI input fields.",
  "authors": [
    "thecsdev",
    "shizotoaster"
  ],
  "contact": {
    "sources": "https://github.com/shizotoaster/mc-ui-input-undo",
    "issues": "https://github.com/shizotoaster/mc-ui-input-undo/issues",
    "homepage": "https://modrinth.com/mod/uiinputundo"
  },
  "license": "GPL v3",
  "icon": "uiinputundo.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "thecsdev.uiinputundo.client.fabric.UIInputUndoClient"
    ]
  },
  "mixins": [
    "uiinputundo.mixins.json",
    "uiinputundo-common.mixins.json"
  ],
  "depends": {
    "fabric": "*",
    "minecraft": ">1.20.1"
  }
}